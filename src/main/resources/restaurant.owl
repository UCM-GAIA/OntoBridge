    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >NoSmoking</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SmokingFacility"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="MalyasianIndonesianCuisine">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Cuisine"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    > </rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="SudaneseCuisine">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    > </rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Cuisine"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Cuisine">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    > The concept cuisine is an abstract concept. all individuals of this concept are first described by a description of the origin and particularities of this cuisine.
	</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Cuisine</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="description"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Rating">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >must be part of another ontology TO BE REMOVED</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Rating</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="FastFoodCuisine">
    <rdfs:subClassOf rdf:resource="#Cuisine"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    > </rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="NewFrenchCuisine">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#FrenchCuisine"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    > </rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="ItalianContemporaryCuisine">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ItalianCuisine"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    > </rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="SpecialFeature">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >RestaurantSpecificFeatures</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This concept has some categories used to identify the specific features proposed by the restaurant such as dancing, biological products etc.
   	</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="AirConditioning">
