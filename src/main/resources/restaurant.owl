      <owl:Class rdf:about="#RestaurantSeating"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="PrivateParking">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
